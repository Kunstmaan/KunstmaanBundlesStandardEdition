{
    "name": "kunstmaan/bundles-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Kunstmaan Bundles Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",

        "kunstmaan/admin-bundle": "2.3.*@dev",
        "kunstmaan/adminlist-bundle": "2.3.*@dev",
        "kunstmaan/article-bundle": "2.3.*@dev",
        "kunstmaan/form-bundle": "2.3.*@dev",
        "kunstmaan/media-bundle": "2.3.*@dev",
        "kunstmaan/media-pagepart-bundle": "2.3.*@dev",
        "kunstmaan/node-bundle": "2.3.*@dev",
        "kunstmaan/node-search-bundle": "2.3.*@dev",
        "kunstmaan/pagepart-bundle": "2.3.*@dev",
        "kunstmaan/search-bundle": "2.3.*@dev",
        "kunstmaan/seo-bundle": "2.3.*@dev",
        "kunstmaan/sitemap-bundle": "2.3.*@dev",
        "kunstmaan/utilities-bundle": "2.3.*",
        "kunstmaan/translator-bundle": "2.3.*@dev",
        "raven/raven": "~0.8",
        "liip/cache-control-bundle": "~1.0.7",
        "ekino/newrelic-bundle": "~1.2.2",

        "knplabs/knp-menu": "2.0.*@dev",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "stof/doctrine-extensions-bundle": "1.1.*@dev",
        "ddeboer/data-import-bundle": "dev-master",
        "fzaninotto/faker": "1.4.*@dev"
    },
    "require-dev": {
        "kunstmaan/behat-bundle": "2.3.*@dev",
        "kunstmaan/generator-bundle": "2.3.*@dev",
        "kunstmaan/live-reload-bundle": "2.3.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanAdminBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanAdminListBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanArticleBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanNodeSearchBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanMediaBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanFormBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanNodeBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanPagePartBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanSearchBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanMediaPagePartBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanSeoBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanSitemapBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanGeneratorBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanBehatBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanLiveReloadBundle.git" },
        { "type": "vcs", "url": "https://github.com/roderik/KunstmaanTranslatorBundle.git" }
    ]
}
